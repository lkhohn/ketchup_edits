  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">

  <script src='public/javascript/navBar.js'></script>

</head>



<body>
  <nav class="navbar navbar-default">
    <div class="container-fluid">
      <div class="navbar-header">
        <a class="navbar-brand" href= <%=linkHome %> >ketchUp</a>
      </div>
      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
        <ul id='nav' class="nav navbar-nav">
          <li class='active'><a href= <%=linkHome %> >home <span class="sr-only"</span></a></li>
          <li><a href= <%=linkApt %> >schedule a meeting</a></li>
        </ul>
        <ul id='nav' class="nav navbar-nav navbar-right">
          <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">account <span class="caret"></span></a>
            <ul id='nav' class="dropdown-menu">
              <li><a href= <%=linkPref %> >preferences</a></li>
              <li role="separator" class="divider"></li>
              <li>
                <a href= <%=linkLogout %>
                  <span class="badge"></span>
                  <span class="glyphicon glyphicon-off" aria-hidden="true"></span> log out</a>
              </li>
            </ul>
          </li>
        </ul>
      </div>
    </div>
  </nav>


      <div class="bigBox">
        <div class="row">
        <div class="leftspace col-md-3">
        </div>
        <div class="signupForm col-md-6">
          <div class="row">
            <div class ="leftBox col-md-4">
            </div>
          <div class="title col-md-4"> WELCOME!!!!  PLEASE LOGIN<br> </div>
          <div class ="leftBox col-md-4">
          </div>
        </div>
          <form action="/login" method="post">
             <div class="signUpBox"><label for="username">Enter a Username<br>
               <input type="text" name="username" placeholder="Put your Username here!"></input>
             </label>
           </div>
             <div class="signUpBox"><label for="password">Enter a Password<br>
               <input type="password" name="password" placeholder="Password123"></input>
             </label>
             </div>
             <br>
              <p>If you don't have an account you can always <a href="signup">Sign Up </a> </p>
             <div class ="submitButton">
             <button type="submit">Submit</button>
           </div>
           </form>
         </div>
         <div class="rightSpace col-md-3">
         </div>
       </div>
     </div>
     <div class ="row">
       </div>

       <div class ="lCoverUp">
       </div>


  <div class="panel panel-default">
    <div class="panel-footer">
      <div class='row'>
        <div class='col-md-6'>
          <ul class='panel-footer-left'>
            <ol><a href= <%=linkHome %> >home </a></ol>
            <ol><a href= <%=linkApt %> >schedule a meeting</a></ol>
            <ol><a href= <%=linkPref %> >preferences</a></ol>
          </ul>
        </div>
        <div class='col-md-6'>
          <ul class='panel-footer-right'>
            <ol>designed by</ol>
            <ol><a href='https://github.com/micah-eberhard'>Micah Eberhard </a></ol>
