  <title></title>

  <link rel="stylesheet" href="/stylesheets/style.css" media="screen" title="no title" charset="utf-8">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">

  <script src='public/javascript/navBar.js'></script>

</head>



<body>
  <nav class="navbar navbar-default">
    <div class="container-fluid">
      <div class="navbar-header">
        <a class="navbar-brand" href= <%=linkHome %> >ketchUp</a>
      </div>
      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
        <ul id='nav' class="nav navbar-nav">
          <li class='active'><a href= <%=linkHome %> >home <span class="sr-only"</span></a></li>
          <li><a href= <%=linkApt %> >schedule a meeting</a></li>
        </ul>
        <ul id='nav' class="nav navbar-nav navbar-right">
          <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><%-username %> <span class="caret"></span></a>
            <ul id='nav' class="dropdown-menu">
              <li><a href= <%=linkPref %> >preferences</a></li>
              <li role="separator" class="divider"></li>
              <li>
                <a href= <%=linkLogout %>
                  <span class="badge"></span>
                  <span class="glyphicon glyphicon-off" aria-hidden="true"></span> log out</a>
              </li>
            </ul>
          </li>
        </ul>
      </div>
    </div>
  </nav>
<!-- END NAVBAR AREA__________________________________________________-->

  <div class="page-header">
    <h1>ketchUp <small>a tool for scheduling meetings</small></h1>
  </div>



<!--beginlink area____________________________-->
<div class="row">
  <div class="loginButton col-md-3">
    <a href="login" class="thumbnail">
      <p class ="text-center" id="loginText">login</p>
    </a>
  </div>
  <div class="signUpButton col-md-3">
    <a href="signup" class="thumbnail">
      <p class="text-center" id="signUpText">sign up</p>
    </a>
  </div>
</div>


  <div class="panel panel-default">
    <div class="panel-footer">
      <div class='row'>
        <div class='col-md-6'>
          <ul class='panel-footer-left'>
            <ol><a href= <%=linkHome %> >home </a></ol>
            <ol><a href= <%=linkApt %> >schedule a meeting</a></ol>
            <ol><a href= <%=linkPref %> >preferences</a></ol>
          </ul>
        </div>
        <div class='col-md-6'>
          <ul class='panel-footer-right'>
            <ol>designed by</ol>
            <ol><a href='https://github.com/micah-eberhard'>Micah Eberhard </a></ol>
            <ol><a href='https://github.com/KeenanHoffman'>Keenan Hoffman </a></ol>
            <ol><a href='https://github.com/lkhohn'>Lindsay Hohn </a></ol>
            <ol><a href='https://github.com/James80401'>James Shield </a></ol>
          </ul>
        </div>
      </div>
    </div>
  </div>

  <div class ="coverUp">
  </div>

  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>

